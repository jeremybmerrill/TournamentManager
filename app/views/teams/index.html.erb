<h1>Listing teams</h1>

<table>
  <tr>
    <th>Amtaid</th>
    <th>School</th>
    <th>Teamabcd</th>
    <th>Contactemail</th>
    <th>Contactphone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.amtaid %></td>
    <td><%= team.school %></td>
    <td><%= team.teamabcd %></td>
    <td><%= team.contactemail %></td>
    <td><%= team.contactphone %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
